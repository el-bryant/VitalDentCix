<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vitaldentcix.vitaldentcix">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Inicio"
            android:label="@string/title_activity_inicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Servicios"
            android:label="@string/title_activity_servicios"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Consulta"
            android:label="@string/title_activity_consulta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Contacto"
            android:label="@string/title_activity_contacto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PacienteAntiguo"
            android:label="@string/title_activity_paciente_antiguo"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".SolicitarCitas" />
        <activity
            android:name=".PacienteNuevo"
            android:label="@string/title_activity_paciente_nuevo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PacienteRegular"
            android:label="@string/title_activity_paciente_regular"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".SolicitarCitas" />
        <activity
            android:name=".QuienesSomos"
            android:label="@string/title_activity_quienes_somos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registro"
            android:label="@string/title_activity_registro"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".SolicitarCitas" />
        <activity
            android:name=".Seleccionar"
            android:label="@string/title_activity_seleccionar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SeleccionarAntiguo"
            android:label="@string/title_activity_seleccionar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SeleccionarRegular"
            android:label="@string/title_activity_seleccionar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SolicitarCitas"
            android:label="@string/title_activity_solicitar_citas"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>